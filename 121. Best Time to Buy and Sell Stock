class Solution {
    public int maxProfit(int[] prices) {
       //by two pointer;
       int ans=0;
       int left=0;
       for(int right=1;right<prices.length;right++){
           if(prices[left]>prices[right]){
            left=right;
           } else{
            ans=Math.max(ans,prices[right]-prices[left]);
           }
       }
       return ans;
    }
}
